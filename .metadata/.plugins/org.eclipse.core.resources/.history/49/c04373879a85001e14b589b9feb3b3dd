package br.com.treinamento;

public class Conta {
	
	//Atributos
	String agencia;
	int numeroConta;
	double saldo;
	
	//Métodos
	void tranferir () {
		
	}
	
	void sacar (double valor) {
		saldo = saldo-valor;
		
		System.out.println("Saque de: R$" +valor);
		
		
	}
	
	void depositar(double valor) {
		saldo = saldo+valor;
		
		System.out.println("Depósito de: R$" +valor);
		
	}

}
